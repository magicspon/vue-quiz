{% macro use(name) %}
	<use class="no-barba" xlink:href="#{{ name }}"></use>
{% endmacro %}

{% macro symbol(name, tag, className, attr) %}
	<{{ tag|default('div') }} class="icon icon--{{ name }} {{ className }}" {{ attr }}>
		<svg>
			<use class="no-barba" xlink:href="#{{ name }}"></use>
		</svg>
	</{{ tag|default('div') }}>
{% endmacro %}


{# example:
	{{ h.input(type='checkbox', name='test', className='form__input' )}}
#}
{% macro input(type, name, className) %}
	<input class="{{ className }}" type="{{ type }}" name="{{ name }}" id="{{ name }}" />
{% endmacro %}


